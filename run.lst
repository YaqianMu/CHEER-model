      v:qkin(j)              i:pk          prod:y(j)        !capital inputs
      v:qlin(j,lm)           i:pl(j,lm)      prod:y(j)        !labor inputs
      v:qlin_ele(sub_elec,lm)      i:pl("elec",lm)      prod:yelec(sub_elec)    
